---
import Layout from "../layouts/Layout.astro";
import AppCards from "../components/AppCards.astro";
import repoData from "../data/repos.json";
export interface Props {
  url: string;
  name: string;
  description: string;
  projectType: string;
}
---

<Layout title="Featured Projects" description="Some of my projects">

  <ul class="app-card-list">
  {
    repoData.map((repoData) => {
      return (
        <AppCards
          name={repoData.name}
          url={repoData.url}
          description={repoData.description}
          projectType={repoData.projectType}
        />
      );
    })
  }
  </ul>
</Layout>
